<?xml version="1.0" encoding="UTF-8"?>

<project name="BuildPipeline-CommitStage" basedir="." default="Packaging">

	<!-- Checkout will be done by CI-Server -->

	<!-- Configuration Section -->
	<property environment="env" />
	<property name="buildversion" value="1.0.0.0-snapshot" />

	<!-- Task Sections Committ Stage -->

	<target name="Init">
		<echo message="Initializing" />
	</target>

	<target name="Compile" depends="Init">
		<echo message="Compile" />
	</target>

	<target name="xUnitTests" depends="Compile">
		<echo message="xUnit testing" />
	</target>

	<target name="Analysis" depends="xUnitTests">
		<echo message="Analysis" />
	</target>

	<target name="Packaging" depends="Analysis">
		<echo message="Packaging" />
	</target>

</project>
